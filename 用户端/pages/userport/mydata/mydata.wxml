<view class='headGroup'>
  <view class='tabBar'>
    <view class="tabItem {{statusType=='ready'?'tabActive':''}}" bindtap='ready'>
      已预约
    </view>
    <view class='line'></view>
    <view class="tabItem {{statusType=='finish'?'tabActive':''}}" bindtap='finish'>
      已完成
    </view>
    <view class='line'></view>
    <view class="tabItem {{statusType=='waitIng'?'tabActive':''}}" bindtap='waitIng'>
      已取消
    </view>
  </view>
</view>
<view class="data-list {{statusType=='ready'?'show':'hide'}}">
  <view class='medicine_item' bindtap='tofinish' data-type='ready'>
    <view class='medicine_img'>
      <image src='/images/4.png'></image>
    </view>
    <view class='medicnie-text'>
      <view class='medicine-tit'>免疫球蛋白</view>
      <view class='style-item'>
        <view class='style-item style-num'>
          <view class='medicine-tips'>剂量：</view>
          <view class='style-detail'>5ml</view>
        </view>
        <view class='style-item'>
          <view class='medicine-tips'>数量：</view>
          <view class='style-detail'>X3</view>
        </view>
      </view>
      <view class='style-item'>
        <view class='medicine-tips'>诊所：</view>
        <view class='style-detail add_detail'>天津市儿童医院</view>
      </view>
      <view class='style-item'>
        <view class='medicine-tips'>预约时间：</view>
        <view class='style-detail add_detail'>2018-04-06 10:00</view>
      </view>
      <view class='style-item'>
        <view class='medicine-tips'>药品总价：</view>
        <view class='style-detail add_detail'>600元</view>
      </view>
    </view>
    <view class='qianw'>
      <view class='date'>
        2018-04-06 10:00
      </view>
      <icon class='iconfont icon-saoma' catchtap='go_code'></icon>
    </view>
  </view>
</view>
<view class="data-list {{statusType=='finish'?'show':'hide'}}">
  <left-smooth bind:del='del'>
    <view class='medicine_item' bindtap='tofinish' data-type='finish'>
      <view class='medicine_img'>
        <image src='/images/4.png'></image>
      </view>
      <view class='medicnie-text'>
        <view class='medicine-tit'>免疫球蛋白</view>
        <view class='style-item'>
          <view class='style-item style-num'>
            <view class='medicine-tips'>剂量：</view>
            <view class='style-detail'>5ml</view>
          </view>
          <view class='style-item'>
            <view class='medicine-tips'>数量：</view>
            <view class='style-detail'>X3</view>
          </view>
        </view>
        <view class='style-item'>
          <view class='medicine-tips'>诊所：</view>
          <view class='style-detail add_detail'>天津市儿童医院</view>
        </view>
        <view class='style-item'>
          <view class='medicine-tips'>预约时间：</view>
          <view class='style-detail add_detail'>2018-04-06 10:00</view>
        </view>
        <view class='style-item'>
          <view class='medicine-tips'>药品总价：</view>
          <view class='style-detail add_detail'>600元</view>
        </view>
      </view>
      <view class='sq'>
        <view class='date'>
          2018-04-06 10:00
        </view>
        <view class='textR'>已完成</view>
      </view>
    </view>
  </left-smooth>
</view>
<view class="data-list {{statusType=='waitIng'?'show':'hide'}}">
  <left-smooth bind:del='del'>
    <view class='medicine_item' bindtap='tofinish' data-type='waitIng'>
      <view class='medicine_img'>
        <image src='/images/4.png'></image>
      </view>
      <view class='medicnie-text'>
        <view class='medicine-tit'>免疫球蛋白</view>
        <view class='style-item'>
          <view class='style-item style-num'>
            <view class='medicine-tips'>剂量：</view>
            <view class='style-detail'>5ml</view>
          </view>
          <view class='style-item'>
            <view class='medicine-tips'>数量：</view>
            <view class='style-detail'>X3</view>
          </view>
        </view>
        <view class='style-item'>
          <view class='medicine-tips'>诊所：</view>
          <view class='style-detail add_detail'>天津市儿童医院</view>
        </view>
        <view class='style-item'>
          <view class='medicine-tips'>预约时间：</view>
          <view class='style-detail add_detail'>2018-04-06 10:00</view>
        </view>
        <view class='style-item'>
          <view class='medicine-tips'>药品总价：</view>
          <view class='style-detail add_detail'>600元</view>
        </view>
      </view>
      <view class='sq'>
        <view class='date'>
          2018-04-06 10:00
        </view>
        <view class='textR'>已取消</view>
      </view>
    </view>
  </left-smooth>
</view>
<toast wrap="{{mask}}" title="删除已完成预约" minTitle="您确认要删除已完成预约么？删除之后将无法恢复。" bind:sure='sure' sureTitle='确定'></toast>
<success wrap="{{delSuccess}}" notice="删除失败"></success>