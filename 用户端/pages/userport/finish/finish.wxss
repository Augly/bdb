.cancle-detail {
  display: flex;
  padding: 12rpx 25rpx;
  border-bottom: 3rpx solid rgba(0, 0, 0, 0.06);
}

.cancle-img {
  width: 225rpx;
  height: 225rpx;
}

.cancle-text {
  margin-left: 45rpx;
}

.cancle-tit {
  font-size: 44rpx;
  margin-top: 25rpx;
  font-family: MicrosoftYaHei;
  font-weight: bold;
  color: #69E0C7;
}
.cancle-tit text{
  font-size: 22rpx;
}
.cancle-tips {
  margin-top: 40rpx;
  font-size: 24rpx;
  font-family: MicrosoftYaHei;
  font-weight: bold;
  color: rgba(153, 153, 153, 1);
}

.cancle-item {
  display: flex;
  height: 112rpx;
  border-bottom: 3rpx solid rgba(241, 241, 241, 1);
  margin: 0 auto;
  width: 690rpx;
  align-items: center;
}

.cancle-deta {
  font-size: 32rpx;
  font-family: MicrosoftYaHei;
  font-weight: bold;
  color: rgba(51, 51, 51, 1);
  width: 200rpx;
  text-align: center;
}

.cancle-data-detail {
  font-size: 24rpx;
  font-family: MicrosoftYaHei;
  font-weight: bold;
  color: rgba(153, 153, 153, 1);
}

.cancle-btn {
  width: 623rpx;
  height: 82rpx;
  background: rgba(153, 153, 153, 1);
  border-radius: 41px;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 34rpx;
  font-family: MicrosoftYaHei;
  font-weight: bold;
  color: rgba(241, 241, 241, 1);
  position: fixed;
  bottom: 60rpx;
  left: 0;
  right: 0;
  margin: 0 auto;
}

.del-success {
  width: 500rpx;
  height: 230rpx;
  background-color: #fff;
  position: relative;
  left: 0;
  right: 0;
  top: 500rpx;
  margin: 0 auto;
  box-shadow: 0px 3rpx 18rpx 3rpx rgba(0, 0, 0, 0.09);
  border-radius: 10rpx;
  overflow: hidden;
}

.del-success-text {
  font-size: 50rpx;
  font-family: Adobe Heiti Std R;
  font-weight: bold;
  color: rgba(253, 73, 120, 1);
  text-align: center;
  margin-top: 90rpx;
}

.tuichu {
  width: 50rpx;
  height: 50rpx;
  position: absolute;
  right: 10rpx;
  top: 10rpx;
}


.icon-riqi{
  color: #92CDC9;
}
.icon-xiaozhensuo{
  color: #594D9C;
}
.icon-yaoping{
  color: #E976A0;
}
.icon-jiliang{
  color: #BF8EBE;
}
.icon-shuliang{
  color: #F4EA2E;
}