page{
  padding-bottom:210rpx; 
}
.cancle-detail {
  display: flex;
  padding: 12rpx 25rpx;
  border-bottom: 3rpx solid rgba(0, 0, 0, 0.06);
}

.cancle-img {
  width: 225rpx;
  height: 225rpx;
}

.cancle-text {
  margin-left: 45rpx;
}

.cancle-tit {
  font-size: 32rpx;
  margin-top: 25rpx;
  font-family: MicrosoftYaHei;
  /* font-weight: bold; */
  color: #69E0C7;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.cancle-tit text{
  font-size: 22rpx;
}
.cancle-tips {
  width: 430rpx;
  margin-top: 20rpx;
  font-size: 24rpx;
  font-family: MicrosoftYaHei;
  /* font-weight: bold; */
  color: rgba(153, 153, 153, 1);
  display: -webkit-box;
-webkit-box-orient: vertical;
-webkit-line-clamp: 3;
overflow: hidden;
}

.cancle-item {
  display: flex;
  height: 112rpx;
  border-bottom: 3rpx solid rgba(241, 241, 241, 1);
  margin: 0 auto;
  width: 690rpx;
  align-items: center;
}

.cancle-deta {
  font-size: 32rpx;
  font-family: MicrosoftYaHei;
  font-weight: bold;
  color: rgba(51, 51, 51, 1);
  width: 200rpx;
  text-align: center;
}

.cancle-data-detail {
  font-size: 24rpx;
  font-family: MicrosoftYaHei;
  /* font-weight: bold; */
  color: rgba(153, 153, 153, 1);
}
.cancleWrap{
  width: 100%;
  position: fixed;
  background-color: rgba(255,255,255,1);
  padding: 60rpx 0rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  bottom: 0;
}
.cancle-btn {
  width: 623rpx;
  height: 82rpx;
  background: rgba(253, 73, 120, 1);
  border-radius: 41px;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 34rpx;
  font-family: MicrosoftYaHei;
  font-weight: bold;
  color: rgba(241, 241, 241, 1);
  /* position: fixed; */
  /* bottom: 60rpx; */
  /* left: 0; */
  /* right: 0; */

}


.del-success {
  width: 500rpx;
  height: 230rpx;
  background-color: #fff;
  position: relative;
  left: 0;
  right: 0;
  top: 500rpx;
  margin: 0 auto;
  box-shadow: 0px 3rpx 18rpx 3rpx rgba(0, 0, 0, 0.09);
  border-radius: 10rpx;
  overflow: hidden
}
.del-success-text{
  font-size:50rpx;
  font-family:Adobe Heiti Std R;
  font-weight:bold;
  color:rgba(253,73,120,1);
  text-align: center;
  margin-top: 90rpx;
}
.tuichu {
  width: 50rpx;
  height: 50rpx;
  position: absolute;
  right: 10rpx;
  top: 10rpx;
}
.title{
  font-size:32rpx;
font-family:MicrosoftYaHei;
font-weight:bold;
color:rgba(51,51,51,1);
}

