<swiper indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}" class='slide-image'>
  <block wx:for="{{imgUrls}}" wx:key=''>
    <swiper-item class='slide-image'>
      <image src="{{item}}" class="slide-image" />
    </swiper-item>
  </block>
</swiper>

<view class='res'>
  <view class='res_list'>
    <span class='iconfont icon-yiyuan1'></span>{{hosRes.name}}
  </view>
  <view class='res_list'>
    <span class='iconfont icon-dianhua1'></span>联系电话（手机）：{{hosRes.tel}}
  </view>
  <view class='res_list'>
    <span class='iconfont icon-weizhi1'></span>{{hosRes.adder}}
  </view>
</view>




<view class='ks'>
  <view class='title'>医院简介</view>
  <view class='produce'>
    {{product}}
  </view>
</view>

