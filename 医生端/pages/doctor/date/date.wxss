.data-list .active::before {
  display: block;
  content: '';
  width: 7rpx;
  height: 97rpx;
  background: rgba(228, 84, 135, 1);
  margin-left: -30rpx;
  margin-right: 30rpx;
}

.medicine_item {
  width: 690rpx;
  border-bottom: 3rpx solid rgba(0, 0, 0, 0.06);
  padding: 17rpx 0;
  display: flex;
  position: relative;
  margin: 0 auto;
  align-items: center;
  /* justify-content: space-between; */
}

.medicine_img {
  width: 120rpx;
  height: 120rpx;
  margin-right: 20rpx;
  border-radius: 50%;
  overflow: hidden;
}

.detail {
  /* position: absolute; */
  font-size: 24rpx;
  font-family: Adobe Heiti Std R;
  font-weight: normal;
  color: rgba(153, 153, 153, 1);
  display: flex;
  align-items: center;
  margin-left: auto;
  /* right: 0; */
}
.detail .iconfont{
  font-size: 24rpx;
}

.detail .iconfont {
  font-size: 24rpx;
}

.medicnie-text {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}

.medicine-tit {
  font-size: 30rpx;
  color: rgba(51, 51, 51, 1);
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
  margin-bottom: 4rpx;
}

.medicine-tips {
  font-size: 24rpx;
  color: rgba(102, 102, 102, 1);
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}

.medicine-data {
  font-size: 22rpx;
  font-family: Adobe Heiti Std R;
  font-weight: normal;
  color: rgba(153, 153, 153, 1);
}

.name-data {
  /* width: 430rpx; */
  display: flex;
  justify-content: space-between;
  align-items: baseline;
}

.dis-flex {
  display: flex;
}

.style-item {
  display: flex;
  margin-bottom: 4rpx;
  line-height: 35rpx;
}

.style-detail {
  font-size: 24rpx;
  color: #666 !important;
}

.add_detail {
  /* width: 300rpx; */
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}

.code {
  width: 64rpx;
  height: 59rpx;
  position: absolute;
  right: 0;
  bottom: 30rpx;
  background-color: red;
}

.finish {
  font-size: 22rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(255, 73, 115, 1);
  position: absolute;
  right: 0;
  bottom: 18rpx;
}

.iconfont {
  font-size: 30rpx;
}

.icon-nan {
  color: RGBA(22, 148, 217, 1);
}

.icon-nv {
  color: RGBA(211, 36, 123, 1);
}

.del {
  width: 128rpx;
  height: 50rpx;
  background: rgba(253, 73, 120, 1);
  border-radius: 10rpx;
  text-align: center;
  line-height: 50rpx;
  position: absolute;
  right: 0;
  bottom: 24rpx;
  font-size: 24rpx;
  font-family: MicrosoftYaHei;
  font-weight: bold;
  color: rgba(241, 241, 241, 1);
}
.mask{
  display: flex;
  align-items: center;
  justify-content: center;
}
.maskWrap{
  width: 500rpx;
  height: auto;
  border-radius:20rpx;
  overflow: hidden;
  background-color: rgba(255, 255, 255,1)
}
.wrapHead{
  width: 100%;
  height: 65rpx;
  background-color: RGBA(253, 73, 120, 1)
}
.wrapContent{
  width: 100%;
  height: auto;
  display: flex;
  box-sizing: border-box;
  flex-direction: column;
  align-items: center;
  padding: 100rpx 70rpx;
}
.wrapTitle{
  font-size:36rpx;
font-family:PingFang-SC-Bold;
font-weight:bold;
color:rgba(102,102,102,1);
}
.wrapMain{
  font-size:24rpx;
font-family:PingFang-SC-Bold;
font-weight:bold;
color:rgba(153,153,153,1);
}
.wrapFoot{
  width: 100%;
  height: auto;
  display: flex;
  align-items: center;
  justify-content:space-around;
  padding-bottom: 55rpx;
}
.foot_sure, .foot_cendel{
  width:149rpx;
height:59rpx;
background:rgba(218,218,218,1);
border-radius:10rpx;
font-size:20rpx;
font-family:PingFang-SC-Bold;
font-weight:bold;
text-align: center;
line-height: 59rpx;
}
.foot_sure{
  background-color: RGBA(253, 73, 120, 1);
  color: rgba(255, 255, 255, 1);
}
.foot_cendel{
  background-color: rgba(218, 218, 218, 1);
  color: rgba(102, 102, 102, 1);
}