.footer-nav {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  width: 100%;
  border-top: 2rpx solid #ccc;
  height: 100rpx;
  background-color: #fff;
  display: flex;
  justify-content: space-around;
}

.footer-nav-item {
  display: flex;
  width: 25%;
  justify-content: center;
  align-items: center;
  flex-direction: column;
}

.footer-img {
  margin-top: 10rpx;
  width: 56rpx;
  height: 56rpx;
  background-color: red;
}

.footer-text {
  margin-top: 8rpx;
  font-size: 20rpx;
  font-family: PingFang-SC-Bold;
  font-weight: bold;
  color: rgba(153, 153, 153, 1);
}

.must-popup {
  width: 500rpx;
  height: 230rpx;
  background: rgba(252, 252, 252, 1);
  box-shadow: 0px 3rpx 18rpx 3rpx rgba(0, 0, 0, 0.09);
  border-radius: 10rpx;
  position: absolute;
  left: 0;
  right: 0;
  top: 50%;
  transform: translateY(-115rpx);
  margin: 0 auto;
}

.close {
  width: 39rpx;
  height: 39rpx;
  position: absolute;
  right: 20rpx;
  top: 20rpx;
}

.must-texf {
  font-size: 50rpx;
  font-family: Adobe Heiti Std R;
  font-weight: bold;
  margin-top: 90rpx;
