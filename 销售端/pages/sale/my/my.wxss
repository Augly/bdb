/* pages/case/case.wxss */

page {
  padding: 0 0 120rpx 0;
}

.notitle {
  position: relative;
  width: 100%;
  height: 220rpx;
  background-color: #63dfc5;
}

.star{
  font-size: 32rpx;
  color: #DB639B;
}
.icon-gerenyonghutouxiang{
  color: #25C173;
}
.icon-weixin{
  color: #1AFA29;
}
.icon-shengribirthday16{
