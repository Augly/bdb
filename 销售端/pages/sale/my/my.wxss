}
.avtarWrap {
  width: 200rpx;
  height: 200rpx;
  border-radius: 50%;
  background-color: rgba(255, 255, 255, 1);
  position: absolute;
  box-sizing: border-box;
  padding: 12rpx;
  top: 120rpx;
  left: 0;
  right: 0;
  margin: 0 auto;
  display: flex;
  align-items: center;
  justify-content: center;
}

.avater {
  width: 100%;
  height: 100%;
  border-radius: 50%;
  background-color: rgba(255, 255, 255, 1);
  /* border: 10rpx solid rgba(255, 255, 255, 1); */
  box-shadow: 0rpx 0rpx 20rpx 4rpx rgba(0, 0, 0, 0.22);
}

.title {
  text-align: center;
  margin-top: 120rpx;
  font-size: 40rpx;
  font-family: MicrosoftYaHei;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
}

.mintitle {
  text-align: center;
  font-size: 24rpx;
  font-family: MicrosoftYaHei;
  font-weight: 400;
  line-height: 60rpx;
  color: rgba(153, 153, 153, 1);
}

.icon-xiangji {
  color: rgba(255, 255, 255, 1);
  position: absolute;
  right: 200rpx;
  bottom: 20rpx;
}

.list {
  width: 100%;
  height: auto;
  box-sizing: border-box;
  padding: 0rpx 30rpx;
  /* margin-top: 100rpx; */
  margin-top: 40rpx;
}

.item {
  width: 100%;
  height: 100rpx;
  /* background-color: red; */
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.left, .right {
  width: 50%;
  height: 100%;
}

.left {
  display: flex;
  align-items: center;
  justify-content: start;
}

.right {
  display: flex;
  align-items: center;
  justify-content: flex-end;
}

.name {
  width: 100%;
  font-size: 24rpx;
  color: rgba(153, 153, 153, 1);
  text-align: right;
}

iconfont {
  font-size: 28rpx;
}

.icon-man-copy {
